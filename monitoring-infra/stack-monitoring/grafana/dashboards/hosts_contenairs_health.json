{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    { "type": "grafana", "id": "grafana", "name": "Grafana", "version": "9.0.0" },
    { "type": "datasource", "id": "prometheus", "name": "Prometheus", "version": "1.0.0" },
    { "type": "panel", "id": "timeseries", "name": "Time series", "version": "" },
    { "type": "panel", "id": "stat", "name": "Stat", "version": "" },
    { "type": "panel", "id": "row", "name": "Row", "version": "" }
  ],
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "type": "row",
      "title": "Host overview",
      "id": 1,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "stat",
      "title": "CPU (containers) - cores used",
      "id": 2,
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 1 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (host) (rate(container_cpu_usage_seconds_total{host=\"$host\",id=~\"/system.slice/docker-.*\\\\.scope\"}[1m]))",
          "legendFormat": "{{host}}"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "horizontal",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "cores",
          "decimals": 2,
          "min": 0
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "CPU (host) - cores total",
      "id": 3,
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 1 },
      "targets": [
        {
          "refId": "A",
          "expr": "count(node_cpu_seconds_total{mode=\"idle\",host=\"$host\"})",
          "legendFormat": "cores"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "horizontal",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "fieldConfig": { "defaults": { "unit": "none", "decimals": 0, "min": 0 }, "overrides": [] }
    },
    {
      "type": "stat",
      "title": "RAM (containers) - working set",
      "id": 4,
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 1 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (host) (container_memory_working_set_bytes{host=\"$host\",id=~\"/system.slice/docker-.*\\\\.scope\"})",
          "legendFormat": "{{host}}"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "horizontal",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "fieldConfig": { "defaults": { "unit": "bytes", "decimals": 1, "min": 0 }, "overrides": [] }
    },
    {
      "type": "stat",
      "title": "RAM (host) - available",
      "id": 5,
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 1 },
      "targets": [
        {
          "refId": "A",
          "expr": "node_memory_MemAvailable_bytes{host=\"$host\"}",
          "legendFormat": "{{host}}"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "horizontal",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "fieldConfig": { "defaults": { "unit": "bytes", "decimals": 1, "min": 0 }, "overrides": [] }
    },

    {
      "type": "row",
      "title": "Top containers",
      "id": 6,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "timeseries",
      "title": "Top CPU containers (rate 1m)",
      "id": 7,
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(10, rate(container_cpu_usage_seconds_total{host=\"$host\",id=~\"/system.slice/docker-.*\\\\.scope\",container_label_com_docker_compose_project=~\"$project\",container_label_com_docker_compose_service=~\"$service\"}[1m]))",
          "legendFormat": "{{container_label_com_docker_compose_project}} / {{container_label_com_docker_compose_service}}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "cores", "decimals": 3, "min": 0 }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "right" } }
    },
    {
      "type": "timeseries",
      "title": "Top RAM containers (working set)",
      "id": 8,
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(10, container_memory_working_set_bytes{host=\"$host\",id=~\"/system.slice/docker-.*\\\\.scope\",container_label_com_docker_compose_project=~\"$project\",container_label_com_docker_compose_service=~\"$service\"})",
          "legendFormat": "{{container_label_com_docker_compose_project}} / {{container_label_com_docker_compose_service}}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "bytes", "decimals": 1, "min": 0 }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "right" } }
    },

    {
      "type": "row",
      "title": "Network & Disk I/O",
      "id": 9,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 14 },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "timeseries",
      "title": "Network RX (sum containers)",
      "id": 10,
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 7, "w": 8, "x": 0, "y": 15 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (host) (rate(container_network_receive_bytes_total{host=\"$host\",id=~\"/system.slice/docker-.*\\\\.scope\"}[1m]))",
          "legendFormat": "{{host}}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "Bps", "decimals": 1, "min": 0 }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } }
    },
    {
      "type": "timeseries",
      "title": "Network TX (sum containers)",
      "id": 11,
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 7, "w": 8, "x": 8, "y": 15 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (host) (rate(container_network_transmit_bytes_total{host=\"$host\",id=~\"/system.slice/docker-.*\\\\.scope\"}[1m]))",
          "legendFormat": "{{host}}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "Bps", "decimals": 1, "min": 0 }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } }
    },
    {
      "type": "timeseries",
      "title": "Disk I/O (containers) - bytes/s (blkio)",
      "id": 12,
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 7, "w": 8, "x": 16, "y": 15 },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (host) (rate(container_blkio_device_usage_total{host=\"$host\",id=~\"/system.slice/docker-.*\\\\.scope\"}[1m]))",
          "legendFormat": "{{host}}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "Bps", "decimals": 1, "min": 0 }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } }
    },

    {
      "type": "row",
      "title": "Docker health",
      "id": 13,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 22 },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "stat",
      "title": "Containers seen (container_last_seen)",
      "id": 14,
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 23 },
      "targets": [
        {
          "refId": "A",
          "expr": "count(container_last_seen{host=\"$host\",id=~\"/system.slice/docker-.*\\\\.scope\"})",
          "legendFormat": "count"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "horizontal",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "fieldConfig": { "defaults": { "unit": "none", "decimals": 0, "min": 0 }, "overrides": [] }
    },
    {
      "type": "stat",
      "title": "Containers stale > 60s",
      "id": 15,
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 23 },
      "targets": [
        {
          "refId": "A",
          "expr": "count((time() - container_last_seen{host=\"$host\",id=~\"/system.slice/docker-.*\\\\.scope\"}) > 60)",
          "legendFormat": "stale"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "horizontal",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "fieldConfig": { "defaults": { "unit": "none", "decimals": 0, "min": 0 }, "overrides": [] }
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["docker", "cadvisor", "node-exporter", "host"],
  "templating": {
    "list": [
      {
        "name": "host",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "refresh": 1,
        "hide": 0,
        "query": {
          "query": "label_values(up{job=~\"dock.*\"}, host)",
          "refId": "PrometheusVariableQueryEditor-Host"
        },
        "includeAll": false,
        "multi": false,
        "sort": 1,
        "current": { "selected": false, "text": "dock02-pwr", "value": "dock02-pwr" }
      },
      {
        "name": "project",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "refresh": 1,
        "hide": 0,
        "query": {
          "query": "label_values(container_cpu_usage_seconds_total{host=\"$host\",container_label_com_docker_compose_project!=\"\"}, container_label_com_docker_compose_project)",
          "refId": "PrometheusVariableQueryEditor-Project"
        },
        "includeAll": true,
        "allValue": ".*",
        "multi": false,
        "sort": 1,
        "current": { "selected": true, "text": "All", "value": "$__all" }
      },
      {
        "name": "service",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "refresh": 1,
        "hide": 0,
        "query": {
          "query": "label_values(container_cpu_usage_seconds_total{host=\"$host\",container_label_com_docker_compose_project=~\"$project\",container_label_com_docker_compose_service!=\"\"}, container_label_com_docker_compose_service)",
          "refId": "PrometheusVariableQueryEditor-Service"
        },
        "includeAll": true,
        "allValue": ".*",
        "multi": false,
        "sort": 1,
        "current": { "selected": true, "text": "All", "value": "$__all" }
      }
    ]
  },
  "time": { "from": "now-6h", "to": "now" },
  "timepicker": { "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h"] },
  "timezone": "browser",
  "title": "Docker - Host by Host (cAdvisor + node-exporter)",
  "version": 1
}
